<div class="row" id="prizes">
	<h1>Prizes</h1>

	<div class = "prize-blocks">
		<% prize_blocks.forEach((prize_block) => { %>
			<div class = "prize-block long-bubble collapsible">
				<img src="images/downTriangle.png" class="faqArrow downTriangle">
				<img src="images/upTriangle.png" class="faqArrow upTriangle">
				<h1><%- prize_block.title %></h1>
				<div class="desc-div">
					<p class="desc"><%- prize_block.desc %></p>
					<p class="prize">Prize: <%- prize_block.prize %></p>
				</div>
			</div>
		<% }) %>
	</div>
</div>

<script>
	var qaToggles = document.getElementsByClassName("collapsible");
	for (var i = 0; i < qaToggles.length; i++) {
		qaToggles[i].addEventListener("click", switchArrows);
	}

	function switchArrows(e) {
		var upTrig, downTrig = null;
		questionChilds = this.childNodes;
		for (var i = 0; i < questionChilds.length; i++) {
			// console.log(questionChilds[i].className);
			if (questionChilds[i].className == "faqArrow upTriangle") {
				upTrig = questionChilds[i];
				// console.log(upTrig);
			}
			else if (questionChilds[i].className == "faqArrow downTriangle") {
				downTrig = questionChilds[i];
				// console.log(downTrig);
			}
		}
		if (upTrig && downTrig) {
			// console.log("toggle");
			if (downTrig.style.display != "none") {
				downTrig.style.display = "none";
				upTrig.style.display = "initial"
			}
			else {
				downTrig.style.display = "initial";
				upTrig.style.display = "none";
			}
		}
	}
</script>